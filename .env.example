# Copy to .env and fill values.
# Never commit your real .env

# Camera
CAMERA_IP=192.168.100.9
CAMERA_USER=admin
CAMERA_PASS=CHANGE_ME

# DVRIP/RTSP ports
DVRIP_PORT=34567
RTSP_PORT=554

# Telegram (optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Alarm pipeline
ALARM_POLL_INTERVAL=300
ALARM_COOLDOWN=5
ALARM_DEBUG_DUMP=0
ALARM_TG_FROM_HISTORY=0
ALARM_TG_REQUIRE_PHOTO=1
ALARM_TG_HISTORY_MAX_AGE_SEC=120
ALARM_EXTRACT_WORKERS=1

# RTSP stability
RTSP_NO_FRAME_TIMEOUT_SEC=15
